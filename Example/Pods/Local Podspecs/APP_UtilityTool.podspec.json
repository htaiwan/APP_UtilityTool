{
  "name": "APP_UtilityTool",
  "version": "0.1.0",
  "summary": "iOS APP Utility Tool.",
  "description": "This is an utility tool for developing iOS app.",
  "homepage": "https://github.com/htaiwan/APP_UtilityTool",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Cheng, Chien-Tai": "htaiwan@gmail.com"
  },
  "source": {
    "git": "https://github.com/htaiwan/APP_UtilityTool.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "4.2",
  "static_framework": true,
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "subspecs": [
    {
      "name": "Ads",
      "dependencies": {
        "Google-Mobile-Ads-SDK": [

        ]
      },
      "source_files": "APP_UtilityTool/Ads/Classes/**/*"
    },
    {
      "name": "Shortcut",
      "frameworks": [
        "Intents",
        "IntentsUI"
      ],
      "source_files": "APP_UtilityTool/Shortcut/Classes/**/*"
    },
    {
      "name": "IAP",
      "frameworks": "StoreKit",
      "source_files": "APP_UtilityTool/IAP/Classes/**/*"
    },
    {
      "name": "Tracking",
      "dependencies": {
        "Firebase/Analytics": [

        ]
      },
      "source_files": "APP_UtilityTool/Tracking/Classes/**/*"
    },
    {
      "name": "Map",
      "frameworks": [
        "CoreLocation",
        "MapKit"
      ],
      "source_files": "APP_UtilityTool/Map/Classes/**/*"
    }
  ],
  "swift_version": "4.2"
}
